{
  "name": "U-Cursedn't",
  "version": "3.7",
  "description": "Esta extensi√≥n agrega funciones adicionales a la plataforma de U-Cursos y respectivos logros por hacer cada una de ellas.",
  "manifest_version": 3,
  "author": "matias-saavedra-g",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "*://*.u-cursos.cl/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/popup.js"]
    },
    {
      "matches": ["*://*.u-cursos.cl/*"],
      "js": ["js/userDataCapture.js", "js/otrasRealizaciones.js"]
    },
    {
      "matches": ["*://*.u-cursos.cl/*/*/*/*/*/notas/*"],
      "js": ["js/easyCopyGrades.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/*/*/*/*/*/integrantes/*",
        "*://*.u-cursos.cl/*/*/*/*/*/*/*/*/integrantes/*"
      ],
      "js": ["js/easyCopyMembers.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/usuario/*/horario/*",
        "*://*.u-cursos.cl/*/*/*/*/*/horario_curso/*"
      ],
      "js": [
        "js/weekCounter.js",
        "js/ical.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/ingenieria/*/*/*/*notas/*"
      ],
      "js": ["js/popupGrading.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://*.u-cursos.cl/*"],
      "js": ["js/resizePreviewPDF.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/ingenieria/2/foro_institucion/*",
        "*://www.u-cursos.cl/ingenieria/*/*/*/*/foro/*",
        "*://www.u-cursos.cl/uchile/4/foro_institucion/",
        "*://www.u-cursos.cl/uchile/*/0/*/1/foro/"
      ],
      "js": ["js/muchoTexto.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/usuario/*",
        "*://*.u-cursos.cl/ucursednt/*",
        "*://*.u-cursos.cl/logros/*"
      ],
      "js": ["js/achievementsBoton.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/logros/*"
      ],
      "js": ["js/achievementsGen.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/usuario/*",
        "*://*.u-cursos.cl/ucursednt/*",
        "*://*.u-cursos.cl/logros/*"
      ],
      "js": ["js/menuBoton.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.u-cursos.cl/ucursednt/*"
      ],
      "js": ["js/menuGen.js", "js/backgroundSettings.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/firstTime.js"]
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/notificationUtils.js", "js/pendingTasks.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/easyCopyCourseDetails.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/renameCourses.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/easterEgg.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/notificationUtils.js", "js/pendingNotifications.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.u-cursos.cl/*"],
      "js": ["js/collapsableMenus.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["popup.html"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["changelog.html"],
      "matches": ["*://www.u-cursos.cl/*"]
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
